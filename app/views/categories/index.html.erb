<h1 align="center">Categories</h1>
<% @categories.each do |category| %>
  <ul class="listing">
    <div class="row">
      <div class="wel col-md-4 col-md-offset-4">
        <li class="article-title">
          <%= link_to "#{category.name}", category  %>
        </li>
      </div>
    </div>
  </ul>
<% end %>
