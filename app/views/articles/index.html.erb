<h1>Articles</h1>
<%= link_to 'Create New Article', new_article_path  %>
<table>
  <tr>
    <th>Title</th>
    <th>Desciption</th>
  </tr>
<% @articles.each do |article| %>
  <tr>
    <td><%= article.title %></td>
    <td><%= article.description %></td>
    <td><%= link_to 'Show', article  %>
    <td><%= link_to 'Edit', edit_article_path(article)  %></td>
  </tr>
<% end %>
</table>
